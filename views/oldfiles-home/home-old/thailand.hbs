<!-- Start Content -->
        <div class="main" role="main">
            <div id="content" class="content full">
                <div class="container">
                    <div class="col-md-12" style="background:#f5f5f5;">
                        <div class="tabs">
                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" href="#countries-packages">Countries packages</a></li>
                                <li class=""><a data-toggle="tab" href="#travel-places">Places</a></li>
                                <li class=""><a data-toggle="tab" href="#travel-tips">Travel Tips</a></li>
                                <li class=""><a data-toggle="tab" href="#travel-contact-us">Contact Us</a></li>
                            </ul>
                            <div class="tab-content">
                                <div id="countries-packages" class="tab-pane active">
                                    {{#each items.packages as |item|}}
                                     <div class="col-md-4">
                                            <div class="grid-item pack-item">
                                                <div class="grid-item-inner">
                                                    <div class="media-box"><a href="/package/{{this._id}}"><img src="{{item.images.[0]}}" alt=""></a></div>
                                                    <div class="grid-content home_package_detail">
                                                        <h3><a href="/package/{{this._id}}">{{this.packagename}}</a></h3>
                                                        <!--<p class="hotel-iten">Honolulu - 3 || Kauai - 2</p>-->
                                                        <div class="price_list">
                                                            <p class="marg-top">
                                                                {{#item.mixWithOccupancies}}
                                                                    {{#roomoccupancyprice}}
                                                                        {{#with option1.[1]}}
                                                                            <sup class="ng-binding">$</sup><span class="room-avg-price ng-binding">{{priceperadult}}</span
                                                                        {{/with}}
                                                                    {{/roomoccupancyprice}}
                                                                {{/item.mixWithOccupancies}}
                                                                <span class="avg-night">Per Person</span>
                                                            </p>
                                                        </div>
                                                        <!--<div class="text-center">
                                                            <p class="btn btn-primary btn_1"><a href="#">VIEW NOW</a></p>
                                                        </div>-->
                                                    </div>
                                                </div>
                                                <div class="ico-back">
                                                    <div class="col-md-8 col-xs-8 pack-icon">
                                                        <img src="images/pack-icons/icon1.svg" alt="">
                                                        <img src="images/pack-icons/icon2.svg" alt="">
                                                        <img src="images/pack-icons/icon3.svg" alt="">
                                                        <img src="images/pack-icons/icon4.svg" alt="">
                                                    </div>
                                                    <div class="col-md-4 col-xs-4 night-info-country">
                                                        <span>{{item.itinerary.length}}</span> Days
                                                    </div>
                                                </div>
                                            </div>
                                    </div>
                                    {{/each}}
                                </div>
                                <div id="travel-places" class="tab-pane">
                                    {{#each items.places as |item|}}
                                        <div class="col-md-4">
                                            <div class="grid-item pack-item">
                                                <div class="grid-item-inner">
                                                    <div class="media-box"><a href="#"><img src="{{item.images.[0]}}" alt=""></a></div>
                                                </div>
                                                <div class="ico-back">
                                                    <div class="col-md-8 col-xs-8 pack-name">
                                                    <h3><a href="#">{{this.placename}}</a></h3>
                                                    </div>
                                                    <div class="col-md-4 col-xs-4 night-info-country">
                                                        <span><a href="#">More</a></span> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {{/each}}
                                </div>
                                <div id="travel-tips" class="tab-pane">
                                    {{#each items.destination as |item|}}
                                    <h4>ESSENTIAL TRAVEL INFORMATION</h4>
                                    <p>{{breaklines this.traveltips}}</p>
                                    <h4>WEATHER &amp; CLIMATE</h4>
                                    <p>{{breaklines this.weather}}</p>
                                    <h4>VISA INFORMATION</h4>
                                    <p>{{breaklines this.visainformation}}</p>
                                    <h4>ETC</h4>
                                    <p>{{this.extraNote}}</p>
                                    {{/each}}
                                </div>
                                <div id="travel-contact-us" class="tab-pane">
                                    <address class="thailand-country-address">
                                        <p>A LUX TRIP THAILAND</p>
                                        <p><b>Working hours:</b> Mon-Fri, 9am-6pm (GMT+7)</p>
                                        <p><b>Address:</b> 942/147-148, Charn Issara Tower 1, Bangkok</p>
                                        <p><b>Tel:</b> +66 226 81280</p>
                                        <p><b>Email:</b> <a href="mailto:thailand@aluxtrip.com"> thailand@aluxtrip.com</a></p>
                                    </address>
                                    <h3 class="country_heading">Send inquiry</h3>
                                    <form id="generalInquiryForm" role="form" data-toggle="validator" method="post" accept-charset="utf-8" novalidate="true">
                                        <label for="firstname">First Name:</label>
                                        <input type="text" name="firstname" placeholder="Enter First Name" class="form-control country_user_name_first" required>
                                        <label for="lastname">Last Name:</label>
                                        <input type="text" name="lastname" placeholder="Enter Last Name" class="form-control country_user_name_last" required>
                                        <label for="email">Email:</label>
                                        <input type="text" name="email" placeholder="Enter Email Id" class="form-control country_user_email" required>
                                        <label for="phonenumber">Phone Number:</label>
                                        <input type="number" name="phonenumber" placeholder="Enter Your phonenumber" class="form-control country_user_number">
                                        <label for="country">Country:</label>
                                        <select id="country" name ="country" class="form-control user_country"></select> 
                                        <label for="inquiry">Inquiry:</label>
                                        <textarea name="inquiry" id="country_inquiry" class="form-control country_inquiry" cols="100" rows="10"></textarea>
                                        <input type="hidden" name="type" value="General" class="form-control country_general_form">
                                        <input type="hidden" name="pagename" value="Thailand Country Page" class="form-control country_general_form_page">
                                        <input type="hidden" name="inquiryStatus" value="New">
                                        <div class="submit_button_group">
                                            <input type="submit" value="SEND INQUIRY" class="btn btn-primary">
                                            <span class="error" style="display:none"> Please Enter Valid Data</span><span  class="success" style="display:none"> Registration Successfully.......... Click To <a href="/thailand" style="color:#0066CC; font-weight:bold">Thailand</a></span>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>